#include <bits/stdc++.h>

using namespace std;

int main() {
   vector<int> v = {1, 7 , 9 ,10, 100, 78 , 71};
   int mx = v[0];
   int mi = v[0];
   for(int i = 0;i<v.size();i++){
       mx = max(mx,v[i]);
       mi = min(mi,v[i]);
   }
   cout<<" min -> " <<mi<<endl;
   cout<<" max -> " <<mx<<endl;
}
